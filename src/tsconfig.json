{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ES2020",
        "moduleResolution": "node",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "static/js",
        "lib": ["ES2020", "DOM", "DOM.Iterable"],
        "types": ["node"]
  },
  "include": ["static/ts/**/*"],
  "exclude": ["src/node_modules"]
}
