{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ES2020",
        "moduleResolution": "node",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "app/static/js",
        "lib": ["ES2020", "DOM", "DOM.Iterable"]
    },
    "include": ["app/static/ts/**/*"],
    "exclude": ["node_modules"]
}